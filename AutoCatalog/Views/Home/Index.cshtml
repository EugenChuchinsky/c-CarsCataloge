@{
    ViewBag.Title = "Автокаталог";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Каталог автомобилей</title>
</head>
<body>
    <a href="/Home/Create">Добавить Авто</a>
    <div>
        <h3>Каталог</h3>
        <table>
            <tr>
                <td><p>Марка</p></td>
                <td><p>Модель</p></td>
                <td><p>Цвет</p></td>
                <td><p>Обьем двигателя</p></td>
                <td><p>Цена</p></td>
                <td><p>Описание</p></td>
                <td></td>
                
            </tr>
            @foreach (var b in ViewBag.Cars)
            {
                <tr>
                    <td><p>@b.Mark</p></td>
                    <td><p>@b.Model</p></td>
                    <td><p>@b.Color</p></td>
                    <td><p>@b.Engine</p></td>
                    <td><p>@b.Price</p></td>
                    <td><p>@b.Description</p></td>
                    
                </tr>
            }
        </table>
    </div>
</body>
</html>